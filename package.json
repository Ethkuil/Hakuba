{
	"name": "svelte-blog",
	"version": "0.0.2",
	"scripts": {
		"dev": "vite dev",
		"generateData": "node --loader ts-node/esm .scripts/pre/index.ts",
		"build": "node --loader ts-node/esm .scripts/pre/index.ts && vite build && node --loader ts-node/esm .scripts/post/index.ts",
		"report": "node --loader ts-node/esm .scripts/pre/index.ts && vite build && ts-node .scripts/post/index.ts",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"source": "node --loader ts-node/esm src/lib/preSource/index.ts"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^6.5.1",
		"@giscus/svelte": "^2.4.0",
		"@playwright/test": "^1.41.2",
		"@sveltejs/adapter-static": "1.0.0-next.48",
		"@sveltejs/kit": "1.0.0-next.372",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/eslint": "^8.21.3",
		"@types/node": "^20.11.20",
		"@types/prettier": "^2.7.3",
		"@types/rollup-plugin-visualizer": "^4.2.3",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"clsx": "^2.1.0",
		"dotenv": "^16.4.5",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"feed": "^4.2.2",
		"lucide-svelte": "^0.510.0",
		"mdsvex": "^0.11.0",
		"node-fetch": "^3.3.2",
		"path-to-regexp": "^6.2.1",
		"postcss": "^8.4.35",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"rehype-add-classes": "^1.0.0",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-external-links": "^3.0.0",
		"rehype-slug": "^6.0.0",
		"remark-footnotes": "2.0",
		"rollup-plugin-visualizer": "^5.12.0",
		"svelte": "^3.59.2",
		"svelte-check": "^3.6.4",
		"svelte-preprocess": "^5.1.3",
		"svelte-sitemap": "^2.6.0",
		"tailwindcss": "^4.1.11",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^2.9.18",
		"yaml": "^2.4.0"
	},
	"type": "module"
}
